Algoritmo calcsimple
	Escribir "Digite el numero 1: ";
	Leer num1;
	Escribir "Digitel el numero2: ";
	Leer num2;
	
	suma<-num1+num2;
	resta<-num1-num2;
	mult<-num1*num2;
	
	Escribir "La suma será igual a: ", suma;
	Escribir "LA resta será igual a: ", resta;
	Escribir "La multiplicacion sera igual a: ", mult;
	
	Escribir "Copyright 2019 by Marie";
FinAlgoritmo
